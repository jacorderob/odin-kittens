<%= form_with model: @kitten do |f| %>
  <%= f.label :name, 'Nombre' %>
  <%= f.text_field :name %>
  <%= f.label :age, 'Edad' %>
  <%= f.number_field :age %>
  <%= f.label :cuteness, 'Ternura' %>
  <%= f.number_field :cuteness %>
  <%= f.label :softness, 'Suavidad' %>
  <%= f.number_field :softness %>
  <%= f.submit 'Guardar' %>
<% end %>